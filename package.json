  "scripts": {
    "watch": "webpack-dev-server --config ./src/config/webpack.development.config.js",
    "build": "npm run build-sw && ./node_modules/.bin/webpack --config ./src/config/webpack.production.config.js",
    "build-sw": "./node_modules/.bin/webpack --config ./src/config/webpack.sw.config.js --mode=production",
